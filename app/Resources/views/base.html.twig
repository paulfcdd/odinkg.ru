<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8"/>
	<title>{% block title %}Welcome!{% endblock %}</title>
	{% block stylesheets %}
		<link href="{{ asset('css/bootstrap.min.css') }}" type="text/css" rel="stylesheet">
		<link href="{{ asset('css/style.css') }}" type="text/css" rel="stylesheet">
	{% endblock %}
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<div class="wrapper">

	<div class="header">
		header
		{#{{ render(url('header')) }}#}
	</div>
	<div class="content">
		<div class="container">
			<div class="row">
				{% block Content %}
				{% endblock %}
			</div>
		</div>
	</div>

	<div class="footer">
		footer
		{#{{ render(url('footer')) }}#}
	</div>

</div>
</body>
</html>
